{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "eqeqeq": "error",
    "curly": "warn",
    "semi": "off",
    "no-console": "warn",
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
